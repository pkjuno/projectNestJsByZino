        $("#submitButton").click(function(){
            const userId = $("#userId").val();
            const password = $("#password").val();

            if(userId === "" && password ===""){
                alert("아이디 /  비밀번호를 입력해주세요");
                return false;
            }else{

            }

        });
    });
</script>
</body>
</html>
